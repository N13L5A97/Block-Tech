<!DOCTYPE html>
<html lang="en">
<head>
  <%- include('../partials/head'); %>
</head>
<body>

<header>
  <%- include('../partials/header'); %>
</header>

<main>
  <div>
    <h2>All Maps</h2>
    <p>You can from all maps here</p>


    <p><%= tagline %></p>

    <ul>
        <% games.forEach(function(game) { %>
          <li>
            <strong><%= game.name %></strong>
           <img src="<%= game.img %>" alt="">,
          </li>
        <% }); %>
      </ul>

  </div>
</main>

<footer>
  <%- include('../partials/footer'); %>
</footer>

</body>
</html>