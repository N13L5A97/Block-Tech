<!DOCTYPE html>
<html lang="en">
<head>
  <%- include('../partials/head'); %>
</head>
<body>
  <header>
  <%- include('../partials/header'); %>
  </header>

  <main class='all'>
    <div>
      <p id="temp"> <%= weather.temp_c %> </p>
      <img src="<%= weather.condition.icon %>" alt="">
    </div>
    <h2>All Maps</h2>
    <ul>  
      <% maps.forEach(function(maps) { %>
        <li>
          <h3> <%= maps.name %> </h3>
          <img src="<%= maps.url %>" alt="">
        </li>
      <% }); %>
    </ul>
  </main>

  <footer>
  <%- include('../partials/footer'); %>
  </footer>
  <script src="../../scripts/script.js"></script>
</body>
</html>