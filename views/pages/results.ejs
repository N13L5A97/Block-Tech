<!DOCTYPE html>
<html lang="en">
<head>
  <%- include('../partials/head'); %>
</head>
<body>

  <header>
    <%- include('../partials/header'); %>
  </header>

  <main class='results'>
    <div>
      <p id="temp"> <%= weather.temp_c %> </p>
      <img src="<%= weather.condition.icon %>" alt="">
    </div>
    <h2>Your Map</h2>
    <section>
      <% maps.forEach(function(maps) { %>
          <h3> <%= maps.name %> </h3>
          <img src="<%= maps.url %>" alt="">
      <% }); %>
    </section>
  </main>
  <footer>
    <%- include('../partials/footer'); %>
  </footer>
</body>
</html>